{
    "name": "udamuri/helpers",
    "description": "Laravel helpers package",
    "keywords": [
        "helpers",
        "terbilang"
    ],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Muri Budiman",
            "email": "udamuri@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Udamuri\\Helpers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Udamuri\\Helpers\\HelpersServiceProvider"
            ]
        }
    }
}
